        "prebuild": "rm -rf dist && mkdir dist dist/assets/",
        "postbuild": "cp -r src/assets/images dist/assets/ && cp -r src/assets/fonts dist/assets/",
        "build": "BUILD=true USE_CONFIG=$npm_config_appConfig ./node_modules/webpack/bin/webpack.js",
        "build:local": "npm run build --appConfig=local",
        "build:production": "npm run build --appConfig=production",
        "dev-server": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js  --config webpack.config.js --open --progress --profile --watch --content-base src/",
        "start": "USE_CONFIG=$npm_config_appConfig ./node_modules/webpack-dashboard/bin/webpack-dashboard.js -- npm run dev-server",
        "start:local": "npm start --appConfig=local",
        "start:production": "npm start --appConfig=production",
        "tslint": "./node_modules/tslint/bin/tslint './src/**/*.ts' -c ./tslint.json -s node_modules/tslint-stylish -t stylish",
        "precommit": "npm run tslint",
        "postinstall": "./node_modules/protractor/bin/webdriver-manager update",
        "protractor": "./node_modules/protractor/bin/protractor ./protractor.config.js",
        "commit": "git-cz",
        "test": "TEST=true ./node_modules/karma/bin/karma start ./karma.config.js"
