{
    "name": "desktime-stats-ng2",
    "version": "0.0.1",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "rm -rf dist && mkdir dist dist/assets/",
        "postbuild": "cp -r src/assets/img dist/assets/",
        "build": "USE_CONFIG=$npm_config_appConfig babel-node --harmony ./node_modules/webpack/bin/webpack --build",
        "build:local": "npm run build --appConfig=local",
        "build:dev-api": "npm run build --appConfig=dev-api",
        "build:production": "npm run build --appConfig=production",
        "start": "USE_CONFIG=$npm_config_appConfig babel-node server.local.js",
        "start:dev-api": "npm run start --appConfig=dev-api",
        "start:local": "npm run start --appConfig=local",
        "start:production": "npm run start --appConfig=production",
        "jshint": "./node_modules/jshint/bin/jshint --reporter ./node_modules/jshint-stylish/ src/app",
        "precommit": "npm run jshint",
        "commit": "git-cz",
        "protractor": "protractor protractor.conf.js",
        "test": "babel-node --harmony ./node_modules/karma/bin/karma start ./karma.conf.js",
        "test-e2e": "./node_modules/protractor/bin/protractor protractor.conf.js",
        "start-dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js  --config webpack.config.js --open --progress --profile --watch --content-base src/"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
